<Window x:Class="SelectFile.StartWindows"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SelectFile"
        mc:Ignorable="d"
        Title="选择功能" Height="563" Width="771" Icon="Images/64642097_p0_master1200.jpg">
    <Grid>
        <Label Content="文件夹重命名" HorizontalAlignment="Left" Margin="10,275,0,0" VerticalAlignment="Top" Height="36" Width="126" FontSize="14" x:Name="PicChange" MouseDown="PicChange_MouseDown" Foreground="#FFC9141C" Panel.ZIndex="999"/>
        <Label Content="壁纸分类" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="31" Width="102" FontSize="14" x:Name="ReNameFolder" MouseDown="ReNameFolder_MouseDown" Panel.ZIndex="999" Background="{x:Null}" Foreground="White"/>
        <Image MouseDown="ReNameFolder_MouseDown" HorizontalAlignment="Left" Height="260" Margin="10,10,0,0" VerticalAlignment="Top" Width="740" Source="Images/64457926_p0.jpg" Stretch="UniformToFill" Cursor="Hand">
        </Image>
        <Image MouseDown="PicChange_MouseDown" HorizontalAlignment="Left" Height="233" Margin="10,275,0,0" VerticalAlignment="Top" Width="740" Source="Images/64619970_p0.jpg" Stretch="UniformToFill" Cursor="Hand"/>

    </Grid>
</Window>
